{
  "name": "cchome-ssr",
  "private": true,
  "description": "This is a Server Side Rendering version for CCW logged out experience. Codename: Trailhead",
  "version": "0.1.0",
  "author": "CCHome",
  "dependencies": {
    "@react/react-spectrum": "^2.33.0",
    "@rearguard/is-bot": "^1.7.0",
    "contentful-resolve-response": "^1.2.2",
    "flow-bin": "^0.118.0",
    "gatsby": "^2.24.74",
    "gatsby-image": "^2.4.21",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-flow": "^1.3.13",
    "gatsby-plugin-intl": "^0.3.3",
    "gatsby-plugin-manifest": "^2.4.34",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sass": "2.3.2",
    "gatsby-plugin-sharp": "^2.6.42",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-source-filesystem": "^2.3.34",
    "gatsby-source-graphql": "^2.7.6",
    "gatsby-transformer-json": "^2.4.14",
    "gatsby-transformer-sharp": "^2.5.17",
    "html-react-parser": "0.13.0",
    "node-sass": "^4.13.1",
    "platform": "^1.3.6",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-intl": "^4.7.6",
    "react-redux": "^7.2.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "prettier": "^1.19.1"
  },
  "scripts": {
    "link": "npm run link-discover-hub",
    "link-discover-hub": "cd node_modules;rm -rf discover-hub;unlink $DISCOVER_HUB_PATH;ln -s $DISCOVER_HUB_PATH",
    "link-learn": "cd node_modules;rm -rf cchome-learn;unlink $LEARN_PATH;ln -s $LEARN_PATH",
    "unlink": "cd node_modules;rm -rf discover-hub;unlink $DISCOVER_HUB_PATH;npm install",
    "build": "gatsby clean && gatsby build --prefix-paths",
    "build-dev": "gatsby clean && gatsby build",
    "develop-analyzed": "BUNDLE_ANALYZER=1 NODE_TLS_REJECT_UNAUTHORIZED=0 gatsby develop -S --port 3000 --host localhost.adobe.com",
    "develop": "NODE_TLS_REJECT_UNAUTHORIZED=0 gatsby develop -S --port 3000 --host localhost.adobe.com",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run build-dev && npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \"",
    "pre-install": "./scripts/pre-install.sh"
  }
}
